	.z80

	include	FCSLIB.INC

	public	?CMP24

	cseg

; Compare two 24-bit values, return CY if BHL > CDE

?CMP24:	ld	a,c
	cp	b
	ret	nz
	ld	a,d
	cp	h
	ret	nz
	ld	a,e
	cp	l
	ret

	end
